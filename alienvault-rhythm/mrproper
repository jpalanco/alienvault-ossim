#!/bin/sh
if [ -f Makefile ]; then
	echo "Making make distclean..."
	make distclean
fi
echo "Removing autogenerated files..."
#rm -rf build-aux
rm -rf autom4te.cache \
    config.guess \
	config.log \
	config.sub \
	configure \
	install-sh \
	missing \
	Makefile \
	Makefile.in \
	./src/Makefile.in \
	./src/Makefile
	
echo "Done."